

<div class="container">
<div class="row justify-content-center">
      <h1>Dishes#index</h1>
      <p>Find me in app/views/dishes/index.html.erb</p>
      <h2>List of Dishes</h2>
</div>
</div>

<div class="container">
      <% @dishes.each do |dish| %>
        <ul>
          <li><%= dish.name %></td>
          <li><%= dish.price %></td>
          <li><%= dish.description %></td>
          <li><%= link_to 'Show', dish_path(dish) %></td>
        </ul>
      <% end %>
</div>
