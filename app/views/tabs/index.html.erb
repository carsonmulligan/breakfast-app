<div class="container">
<div class="row justify-content-center">
      <h1>Tabs#index</h1>
      <p>Find me in app/views/tabs/index.html.erb</p>
      <h2>List of tables</h2>
</div>
</div>

<div class="container">
      <% @tabs.each do |tab| %>
        <tr>
          <td><%= tab.table_number %></td>
          <td><%= tab.status %></td>
          <td><%= link_to 'Show', tab_path(tab) %></td>
        </tr>
      <% end %>
</div>
